<%= simple_form_for @member,:html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name_member %>
    <%= f.input :social_reason %>
    <%= f.input :address %>
    <%= f.input :rfc %>
    <%= f.input :comercial_key %>
    <%= f.input :phone %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
